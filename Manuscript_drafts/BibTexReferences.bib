@article{morgan_spatial_2000-1,
	title = {Spatial variability in red sea urchin ({Strongylocentrotus} franciscanus) recruitment in northern {California}},
	volume = {9},
	abstract = {To better understand the spatial distribution of recruitment in the northern California red sea urchin (Strongylocentrotus franciscanus) population, we sam- pled size distributions at each of 12 locations in 1995 and 1996, two of those locations in 1994, and 5 of those locations in 1997. An index of recent recruit- ment in these size distributions and an estimate of density of recent recruitment re Ì„ect a similar spatial pattern of recruitment. This pattern appears to be determined by the effect of coastal circulation on larval delivery during relaxation of upwelling, and not the result of the positive effect of the adult spine canopy on juvenile survival. Recent recruitment of red sea urchins in northern California is higher in areas more frequently subjected to onshore and poleward  Ì„ow during relaxation of upwelling. These results are consistent with a mechanism by which alongshore spatial variability in southward, offshore  Ì„ow during upwelling winds allows larvae to maintain latitudinal position, whereas  Ì„ows during event-scale relaxations in upwelling winds serve to distribute settling larvae alongshore, favouring areas north of promontories. The consequent spatial pattern of red sea urchin set- tlement varies from year to year, and there are not yet sufÂ®cient data to demonstrate the degree to which this spatial pattern in recruitment determines a spatial pattern in Â®shable adult abundance.},
	journal = {Fisheries Oceanography},
	author = {Morgan, Lance E. and Wing, Stephen R. and Botsford, Louis W. and Lundquist, Carolyn J. and Diehl, Jennifer M.},
	year = {2000},
	pages = {83--98}
}

