@article{gerber_including_2006,
	title = {Including behavioral data in demographic models improves estimates of population viability},
	volume = {4},
	abstract = {Individual differences in behavior and social status can determine the rate of population change and thus the threat that imperiled species face in the real world. One way that behavior may be manifested in terms of an annualized population growth rate (Â’) is through the effects of sex ratio biases. A populationâ€™s operational sex ratio (OSR) typically deviates from the actual sex ratio (ASR), leading to pronounced sexual dimorphism in fecundity. This paper provides a framework for integrating behavioral estimates of OSR into population models used for conservation. Male and female fertility is estimated at six island study sites for California sea lions (Zalophus californianus), using population census data (ASR) and behavioral data (OSR). These estimates are used to construct alternate projection matrices to examine the impacts on Â’. Inclusion of behavioral data in population models not only allows for explicit analysis of the effects of behaviors on viability, but may also help to identify behavioral attributes that could be used as an early indicator of population decline.},
	number = {8},
	journal = {Frontiers in Ecology and the Environment},
	author = {Gerber, Leah R.},
	year = {2006},
	pages = {419--427}
}

@article{hettinger_persistent_2012,
	title = {Persistent carry-over effects of planktonic exposure to ocean acidification in the {Olympia} oyster},
	volume = {93},
	abstract = {Predicting impacts of global environmental change is challenging due to the complex
20 life cycles that characterize many terrestrial and aquatic taxa. Different life stages often interact
21 with the physical environment in distinct ways, and a growing body of work suggests that
22 stresses experienced during one life stage can â€œcarry overâ€ to influence subsequent stages.
23 Assessments of population responses to environmental perturbation must therefore consider how
24 effects might propagate across life-history transitions. We investigated consequences of ocean
25 acidification (decreased pH and carbonate saturation) for early life stages of the Olympia oyster
26 (Ostrea lurida), a foundation species in estuaries along the Pacific coast of North America. We
27 reared oysters at three levels of seawater pH, including a control (8.0) and two additional levels
28 (7.9 and 7.8). Oysters were cultured through their planktonic larval period to metamorphosis and
29 into early juvenile life. Larvae reared under pH 7.8 exhibited a 15\% decrease in larval shell
30 growth rate, and a 7\% decrease in shell area at settlement, compared to larvae reared under
31 control conditions. Impacts were even more pronounced a week after settlement, with juveniles
32 that had been reared as larvae under reduced pH exhibiting a 41\% decrease in shell growth rate.
33 Importantly, the latter effect arose regardless of the pH level the oysters experienced as juveniles,
34 indicating a strong carry-over effect from the larval phase. Adverse impacts of early exposure to
35 low pH persisted for at least 1.5 months after juveniles were transferred to a common
36 environment. Overall, our results suggest that a stringent focus on a single phase of the life cycle
37 (e.g., one perceived as the â€œweakest linkâ€), may neglect critical impacts that can be transferred
38 across life stages in taxa with complex life histories.},
	number = {12},
	journal = {Ecology},
	author = {Hettinger, Annaliese and Sanford, Eric and Hill, Tessa M. and Russell, Ann D. and Sato, Kirk N.S. and Hoey, Jennifer and Forsch, Margaux and Page, Heather N. and Gaylord, Brian},
	year = {2012},
	keywords = {carry-over effects, ocean acidification, Olympia oyster, Ostrea lurida},
	pages = {2758--2768}
}

